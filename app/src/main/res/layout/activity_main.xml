<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.qy.q233.MainActivity">
    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical">
        <Button
            android:id="@+id/button_jump1"
            android:layout_width="368dp"
            android:layout_height="wrap_content"
            android:text="@string/Jump1" />
        <Button
            android:id="@+id/button_jump2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/Jump2"/>
        <Button
        android:id="@+id/button_jump3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/Jump3"/>

        <Button
            android:id="@+id/button_jump4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Jump4"/>
    </LinearLayout>


</android.support.constraint.ConstraintLayout>
